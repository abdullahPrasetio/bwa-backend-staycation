<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="nav-home-tab">
    <div class="row">
        <% for(let x=0;x<item.imageId.length; x++) {%>
            <div class="col-md-4">
                <div class="card shadow mb-4 mt-2">
                    <div class="card-body">
                        <img src="https://admin-staycation-id.herokuapp.com/<%=item.imageId[x].imageUrl %> " class="img-fluid ${3|rounded-top,rounded-right,rounded-bottom,rounded-left,rounded-circle,|}" alt="image" style="width:100%; display:block">
                    </div>
                </div>
            </div>
        <% } %> 
    </div>
    <a href="/admin/item" class="btn btn-warning">Back</a>
</div>